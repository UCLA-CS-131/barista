{
  "name": "barista",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-simple-code-editor": "^0.13.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-api": "venv/bin/flask run --port 8000",
    "build": "react-scripts build",
    "format": "prettier src --write",
    "lint": "npm-run-all --parallel --continue-on-error lint:*",
    "lint:js": "eslint src --cache --max-warnings=0 --ext .js,.ts,.tsx",
    "lint:types": "tsc",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.4",
    "tailwindcss": "^3.2.7"
  }
}
